{
  "kind": "collectionType",
  "collectionName": "blog_post_types",
  "info": {
    "singularName": "blog-post-type",
    "pluralName": "blog-post-types",
    "displayName": "Blog Post Type",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "description": {
      "type": "text"
    },
    "blog_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "blog_post_type"
    },
    "rank": {
      "type": "integer"
    }
  }
}
